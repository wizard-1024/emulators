; Загрузка ИС-2, к/с для ИС-2 и вызов СП 36 с печатью результатов
; (использует готовые образы МБ из is3_build)
; (2015 Стефанков)
;
! del is3_test_0016_debug.txt
! del is3_test_0016.lst
;
set console debug=is3_test_0016_debug.txt
;de DEBUG_DUMP_REGS 1
;de DEBUG_DUMP_MEM  1
set cpu debug
set drum debug
set cdr  debug
set lpt  debug
set cdp  debug
set mt   debug
;
;de DPTYPE 4
de DPTYPE 2
set lpt OCTHELPFMT
;set lpt OCTHELPFMT
;
de DRUM_MAP_CHECK 1
de LOG_DRUM_1_MAP 0
de LOG_DRUM_0_MAP 1
de DRUM_0_ACCESS_MODE 1
;
att lpt   is3_test_0016.lst
att drum0 is3_test_0016.drum0
;att drum1 is3_test_0016.drum1
;att drum2 is3_test_0016.drum2
;
;de USE_NEW_ADD 1
;de ROUND_ERROR_BITS_OFF 0
;de USE_NEW_MULT 1
;de USE_NEW_DIV 1
;de USE_NEW_SQRT 1
;
load is3_test_0016.m20
;
ex 100-127
echo
ex -m 100-127
echo
ex 20-25
echo
ex 30-31
echo
echo Start
run
show queue
show time
;show throttle
ex 30-31
echo
ex 100-127
echo
ex -m 100-127
echo
ex 0-3
echo
ex 20-25
echo
;ex 7544-7544
;echo
;ex 7200-7230
quit
