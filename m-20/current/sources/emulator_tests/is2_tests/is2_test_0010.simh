; Загрузка ИС-2, к/с для ИС-2 и вызов СП 42 с печатью результатов (10 раз)
; (использует готовые образы МБ из is2_build)
; (2015 Стефанков)
;
! del is2_test_0010_debug.txt
! del is2_test_0010.lst
;
set console debug=is2_test_0010_debug.txt
;de DIAG_PRINT 1
;de DEBUG_DUMP_REGS 1
;de DEBUG_DUMP_MEM  1
;de ARITHMETIC_OP_DEBUG 1
set cpu debug
set drum debug
set cdr  debug
set lpt  debug
set cdp  debug
set mt   debug
de DPTYPE 2
;de DPTYPE 4
set lpt OCTHELPFMT
att lpt   is2_test_0010.lst
att drum0 is2_test_0010.drum0
att drum1 is2_test_0010.drum1
att drum2 is2_test_0010.drum2
;
;de USE_NEW_ADD 1
;de ROUND_ERROR_BITS_OFF 1
;de ROUNDING_UP_ON 0
;de USE_NEW_MULT 1
;de USE_NEW_DIV 1
;de USE_NEW_SQRT 1
;
load is2_test_0010.m20
;
ex 100-137
echo
ex -m 100-137
echo
ex 20-32
echo
ex 35-36
echo
echo Start
run
show queue
show time
;show throttle
ex 35-35
echo
ex 100-137
echo
ex -m 100-137
echo
ex 0-3
echo
ex 20-32
echo
ex 35-36
echo
;ex 7544-7544
;echo
;ex 7200-7230
quit
