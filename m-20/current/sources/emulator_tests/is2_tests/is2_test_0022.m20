;



:0100			; Команды
;
0 50 0013 7000 7767	; Загружаем ИС-2
0 70 7500 0000 0000
;
0 16 0103 7500 7610	; Вызов СП 00
0 13 0200 2310 0246	; УЧ, начало_МОЗУ, адрес_МЗУ, конец_МОЗУ
;
0 77 0000 0000 0000 	; Остановка
7 77 7777 7777 7777     ; останов после выдачи

@0100			; Старт
