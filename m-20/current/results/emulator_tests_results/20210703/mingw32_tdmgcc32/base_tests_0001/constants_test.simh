; Тест верности констант (2015 Стефанков)
;
! del constants_test.txt
! del constants_test.lst
;
;set console debug=constants_test.txt
;de DEBUG_DUMP_REGS 1
;de DEBUG_DUMP_MEM  1
;de DEBUG_DUMP_MODERM_MEM 1
;de ARITHMETIC_OP_DEBUG 1
;set cpu debug
;set drum debug
;set cdr  debug
;set lpt  debug
;set cdp  debug
;set mt   debug
de DPTYPE 2
set lpt OCTHELPFMT
att lpt constants_test.lst
;
;de USE_ADD_SBST 1
;de USE_NEW_ADD 0
;de ROUND_ERROR_BITS_OFF 0
;de ROUNDING_UP_ON 0
;de USE_NEW_MULT 1
;de USE_NEW_DIV 1
;de USE_NEW_SQRT 1
;
load constants.m20
load constants_test.m20
;
ex 100-107
echo
ex -m 100-107
echo
ex 1000-1144
echo
echo Start
run
show queue
show time
;show throttle
quit
